{
  "name": "alexmazorchuk/serialize-test",
  "description": "This is a test serialize lib for homework PHP projects.",
  "keywords": [
    "sort",
    "library",
,    "serialize"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Mazorchuk",
      "email": "019maz@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "symfony/yaml": "^4.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "Alexmazorchuk\\Sort\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alexmazorchuk\\Sort\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
