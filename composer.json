{
  "name": "alexmazorchuk/serialize-test",
  "description": "This is a test serialize lib for homework PHP projects.",
  "keywords": [
    "default-project",
    "serialize"
  ],
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Mazorchuk",
      "email": "019maz@gmail.com",
      "role": "Maintainer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alexmazorchuk/serialize-test"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}

